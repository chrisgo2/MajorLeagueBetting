<div id="profileFeed">
<h1>Users#show</h1>
<p>Find me in app/views/users/show.html.erb</p>

  This is the Profile for <%= current_user.name %>
</div>



  <%= form_for(current_user, :html => { :multipart => true }) do |f| %>
   <%= render 'shared/validation', :object => f.object %>
    <div class="label">
      <%= f.label      :avatar %>
    </div>
    <td>
      <%= f.text_field :name %>
      <%= f.file_field :avatar %>
    </td>
    <div class="btn">
     <%= f.submit "Upload" %>
    </div>
  <% end %>
  

